@model IEnumerable<MSWebPROJ2.Models.User>


@{
    ViewData["Title"] = "User List";
}

<h1>User List</h1>

<table class="table">
    <thead>
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.FirstName</td>
                <td>@user.LastName</td>
                <td>
                    <a asp-page="/DbUpdate" asp-route-id="@user.ID">Edit</a>
                </td>
            </tr>
        }
    </tbody>
    </table>
